{
  "extends": [ "standard", "google" ],
  "plugins" : [
    "google-camelcase"
  ],
  "rules": {
    "no-warning-comments": "off",
    "camelcase": "off",
    "google-camelcase/google-camelcase": "error",
    "no-lonely-if": "warn",
    "no-else-return": "warn"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
