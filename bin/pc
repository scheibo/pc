#!/usr/bin/env node

'use strict';

var pc = require('../');

// TODO(kjs): actually do stuff.

/** Expose / Entry Point */
if (!module.parent) {
  process.title = 'pc';
  main(process.argv.slice(), function(err, code) {
    if (err) throw err;
    return process.exit(code || 0);
  });
} else {
  module.exports = main;
}
