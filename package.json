  "main": "./lib/pc.js",
  "bin": "./bin/pc",
  "keywords": ["pace", "calculator"],
  "dependencies": {
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "coveralls": "^2.11.9",
    "eslint": "^2.10.2",
    "eslint-config-google": "^0.5.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-google-camelcase": "^0.0.2",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5"
  },
  "files": [
    "LICENSE",
    "README.md",
    "bin",
    "index.js",
    "lib"
  ],
  "scripts": {
    "lint": "eslint lib/** bin/** test/** index.js; exit 0",
    "test": "mocha --reporter spec --check-leaks test/",
